<h3>Member: <%= "#{@member.first_name} #{@member.last_name}" %></h3>

<p><%= link_to "Select a Member", members_path %></p>

<div id="outta_face">
  <p style="color:red">OUTTA:</p>
  <p>
    <%= form_tag("/outtamyface", :method => "get") do %>
      <%= text_field_tag(:face) %>
      <%= hidden_field_tag(:id, params[:id]) %>
      <%= submit_tag("Create") %>
    <% end %>
    <% if @member.outta.any? %>
      <%= nested hash_outta(@member.username) %>
    <% end %>
  </p>
</div>

<div id="into_face">
  <p style="color:blue">IN:</p>
  <p>
    <%= form_tag("/inmyface", :method => "get") do %>
      <%= text_field_tag(:face) %>
      <%= hidden_field_tag(:id, params[:id]) %>
      <%= submit_tag("Create") %>
    <% end %>
    <% if @member.into.any? %>
      <%= nested hash_into(@member.username) %>
    <% end %>
  </p>
</div>

<span class="clear">