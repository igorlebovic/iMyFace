<h3>New Member</h3>

<%= form_tag("/members", :method => "post") do %>
  <p>
  <%= label_tag(:first_name, "First Name:") %></br>
  <%= text_field_tag(:first_name) %>
  </p>
  <p>
  <%= label_tag(:last_name, "Last Name:") %></br>
  <%= text_field_tag(:last_name) %>
  </p>
  <p>
  <%= label_tag(:username, "Username:") %></br>
  <%= text_field_tag(:username) %>
  </p>
  <p>
  <%= label_tag(:password, "Password:") %></br>
  <%= password_field_tag(:password) %>
  </p>
  <p>
  <%= label_tag(:outta, "OuttaMyFace:") %></br>
  <%= text_field_tag(:outta) %>
  </p>
  <p>
  <%= label_tag(:into, "IntoMyFace:") %></br>
  <%= text_field_tag(:into) %>
  </p>
  <p>
  <%= submit_tag("Create Member") %>
  </p>
<% end %>